#!/bin/bash
#SBATCH -p fat
#SBATCH --mail-type=All
#SBATCH --mail-user=zaheer.phd.zool@pu.edu.pk
#SBATCH --nodes=1
#SBATCH --ntasks=16
#SBATCH --mem=50G
#SBATCH -t 0-01:30:00
#SBATCH -o sct-%J.out
#SBATCH -e sct_err-%J.out

